{
  "name": "cookie-auth-with-supabase",
  "displayName": "Cookie auth with supabase",
  "version": "0.0.1",
  "description": "An extension that uses cookies and supabase to manage auth via a separate website",
  "author": "oisin_thomas",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@plasmohq/storage": "1.9.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "cookies"
    ]
  }
}
